rn {\n            page : 1,\n        };\n    },\n    watch : {\n        value : function (val, oldVal) {\n            this.page = +val;\n        },\n        page : function (val, oldVal) {\n            if (val < 1) {\n                this.page = 1;\n                return;\n            }\n            if (val > this.pagesCnt) {\n                this.page = +this.pagesCnt;\n                return;\n            }\n            this.$emit('input', this.page);\n        },\n        pagesCnt : function (val, oldVal) {\n            this.page = 1;\n        },\n    },\n}\n</script>\n"]}]}